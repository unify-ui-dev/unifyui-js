{
  "name": "@unifyui-js/helpers",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "description": "A simple Helper packages, containing all usefull function for all @unifyui-js packages",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/helpers.umd.cjs",
  "module": "./dist/helpers.js",
  "types": "./dist/index.d.ts",
  "exports":{
    ".":{
      "import":"./dist/helpers.js",
      "require":"./dist/helpers.umd.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "!uno.config.ts",
    "!tsconfig.json",
    "!vite.config.json",
    "package.json"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@types/node": "^20.10.6",
    "rollup-plugin-dts": "^6.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.0"
  },
  "keywords": [
    "unifyui-js",
    "helpers",
    "component"
  ],
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/unify-ui-dev/unifyui-js.git"
  },
  "bugs": {
    "url": "https://github.com/unify-ui-dev/unifyui-js/issues"
  },
  "license": "MIT",
  "author": "johnkat-mj"
}
